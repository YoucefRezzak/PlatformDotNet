<Window x:Class="Views.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Views"
        mc:Ignorable="d"
        Title="Menu" Height="200" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="27*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Upload Files" HorizontalAlignment="Left" Margin="37,71,0,0" VerticalAlignment="Top" Width="74" Height="24" RenderTransformOrigin="0.5,0.5" Click="Upload"/>
        <Button Content="Send Files" HorizontalAlignment="Left" Margin="151,71,0,0" VerticalAlignment="Top" Width="74" Height="24" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2" Click="send"/>
        <Button Content="Show Results" HorizontalAlignment="Left" Margin="82,71,0,0" VerticalAlignment="Top" Width="74" Height="24" RenderTransformOrigin="0.5,0.5" Grid.Column="1" Click="result"/>

    </Grid>
</Window>
